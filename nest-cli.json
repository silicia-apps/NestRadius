{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/fr4dius/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/fr4dius/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/fr4dius",
  "projects": {
    "fr4dius": {
      "type": "application",
      "root": "apps/fr4dius",
      "entryFile": "main",
      "sourceRoot": "apps/fr4dius/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fr4dius/tsconfig.app.json"
      }
    },
    "rlm-uisp": {
      "type": "application",
      "root": "apps/rlm-uisp",
      "entryFile": "main",
      "sourceRoot": "apps/rlm-uisp/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rlm-uisp/tsconfig.app.json"
      }
    }
  }
}